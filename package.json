{
  "name": "vjs-query",
  "version": "1.0.0-alpha.1",
  "description": "Vanilla JS query",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uk-taniyama/vjs-query.git"
  },
  "author": "uk-taniyama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uk-taniyama/vjs-query/issues"
  },
  "homepage": "https://github.com/uk-taniyama/vjs-query#readme",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "doc": "typedoc",
    "test": "jest",
    "test-watch": "jest --watch src",
    "test-coverage": "jest --coverage",
    "lint": "eslint .eslintrc.cjs jest.config.cjs rollup.config.mjs src",
    "lint-fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/jest-image-snapshot": "^6.2.0",
    "@types/jsdom": "^21.1.2",
    "@types/json-schema": "^7.0.12",
    "@types/lodash-es": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "dts-merge-interface": "^1.0.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-html-reporters": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.0",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  }
